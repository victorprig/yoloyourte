<% if current_user %>
<%= link_to "See Profile", profile_path(current_user.profile), :method => :get, class: 'btn btn-info full-width' %>
<%= link_to('Logout', destroy_user_session_path, :method => :delete, class: 'btn btn-info full-width') %>
<%= link_to "My yurts", yurts_path, class: 'btn btn-info full-width' %>
<% else %>
<%= link_to('Login', new_user_session_path, class: 'btn btn-info full-width')  %>
<%= link_to("Sign Up", new_user_registration_path, :method => :get, class: 'btn btn-info full-width') %>
<% end %>

